! URXVT Configuration !

URxvt.scrollBar:    false    ! Turn the scrollbar off!
URxvt.font:xft:Droid Sans Mono:pixelsize=12
Xft.dpi: 96
Xft.antialias:true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
URxvt.letterSpace:-1





